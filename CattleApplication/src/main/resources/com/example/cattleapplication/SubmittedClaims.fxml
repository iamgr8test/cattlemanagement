<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.cattleapplication.SubmittedClaimsController">
    <children>
        <TableView fx:id="submittedClaimsTable" prefWidth="600" prefHeight="400">
            <columns>
                <TableColumn fx:id="claimIdColumn" text="Claim ID" />
                <TableColumn fx:id="cattleIdColumn" text="Cattle ID" />
                <TableColumn fx:id="claimStatusColumn" text="Claim Status" />
                <TableColumn fx:id="submittedByColumn" text="Submitted By" />
                <TableColumn fx:id="submissionDateColumn" text="Submission Date" />
            </columns>
        </TableView>
        <Button text="Back to Dashboard" onAction="#goBackToDashboard" layoutX="250" layoutY="360"/>
    </children>
</AnchorPane>
