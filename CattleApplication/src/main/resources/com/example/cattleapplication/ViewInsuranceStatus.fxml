<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cattleapplication.ViewInsuranceStatusController">
    <children>
        <Label text="Enter Cattle ID:" layoutX="30.0" layoutY="30.0"/>
        <TextField fx:id="cattleIdTextField" layoutX="150.0" layoutY="30.0" />
        <Button text="Check Status" layoutX="150.0" layoutY="70.0" onAction="#handleCheckStatus"/>
        <Label fx:id="statusLabel" layoutX="30.0" layoutY="120.0" wrapText="true" />
        <Button text="Back to Dashboard" layoutX="30.0" layoutY="300.0" onAction="#handleBackToDashboard"/>
    </children>
</AnchorPane>
