<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cattleapplication.SubmitInsuranceClaimController" spacing="10" alignment="CENTER">
    <Label text="Submit Insurance Claim" />

    <ComboBox fx:id="insurancePolicyComboBox" />
    <ComboBox fx:id="cattleIdComboBox" />

    <Button text="Submit Claim" fx:id="submitClaimButton" onAction="#handleSubmitClaim"/>
    <Button text="Back to Dashboard" onAction="#handleBackToDashboard"/>
</VBox>
